{% extends 'base.html.twig' %}

{% block title %}Photos{% endblock %}

{% block body %}
<div class="home">
    <h1 class="title">50 anni Emanuela</h1>
    
    <div class="owl-carousel">
        {% for item in list %}
            <div class="photos" key={index}>
                <img src={{ asset('photos/'~item)}}></img>
            </div>    
        {% endfor %}
    </div>
</div>
{% endblock %}
